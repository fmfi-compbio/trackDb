        labelFields acc,uniprotName,geneName,hgncSym,refSeq,refSeqProt,ensProt,uniprotName
        mouseOverField protFullNames
        itemRgb off
        color 0,150,250
        indelDoubleInsert on
        indelQueryInsert on
        baseColorTickColor contrastingColor

