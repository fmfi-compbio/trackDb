################################################################
# macEug2 - Wallaby - Macropus eugenii - mammal - mammal - 3610
################################################################
track chainNetMacEug2
release alpha
compositeTrack on
shortLabel $o_Organism Chain/Net
longLabel $o_Organism ($o_date), Chain and Net Alignments
subGroup1 view Views chain=Chain net=Net
dragAndDrop subTracks
visibility hide
group compGeno
noInherit on
priority 200.3
color 0,0,0
altColor 255,255,0
type bed 3
sortOrder view=+
matrix 16 91,-170,-39,-185,-170,100,-50,-39,-39,-50,100,-170,-185,-39,-170,91
matrixHeader A, C, G, T
otherDb macEug2
chainMinScore 3000
chainLinearGap medium
spectrum on
html chainNet

    track chainNetMacEug2Viewchain
    release alpha
    shortLabel Chain
    view chain
    visibility pack
    parent chainNetMacEug2
    spectrum on

        track chainMacEug2
        release alpha
        parent chainNetMacEug2Viewchain
        subGroups view=chain
        shortLabel $o_Organism Chain
        longLabel $o_Organism ($o_date) Chained Alignments
        type chain macEug2
        html chainNet

    track chainNetMacEug2Viewnet
    release alpha
    shortLabel Net
    view net
    visibility dense
    parent chainNetMacEug2

        track netMacEug2
        release alpha
        parent chainNetMacEug2Viewnet
        subGroups view=net
        shortLabel $o_Organism Net
        longLabel $o_Organism ($o_date) Alignment Net
        type netAlign macEug2 chainMacEug2
        html chainNet

